-- SHORTENED 

SELECT *
FROM shopping
WHERE NOT (
    (
        shopping_website_type IN (
            'alcohol', 'dating', 'escort', 'explosives', 
            'financial trading platform', 'gambling', 'government id',
            'intellectual property', 'other financial risk', 'payments', 
            'registrar', 'registry', 'rx eyewear', 'telemarketing firm', 
            'travel', 'virtual currency', 'weapons', 'wildlife parts'
        )
        OR shopping_website_type ILIKE '%debt servicing%'
        OR shopping_website_type ILIKE '%hate%'
        OR shopping_website_type ILIKE '%loans%'
    )
    AND (
        (secondary_website_type NOT LIKE '%cbd%')
        AND (secondary_website_type NOT ILIKE '%marijuana%')
        AND (secondary_website_type NOT ILIKE '%cosmetic%' )
        AND (secondary_website_type NOT ILIKE '%pharmacy%')
        AND (secondary_website_type NOT ILIKE '%other healthcare%')
        AND (secondary_website_type NOT ILIKE '%paraphernalia%')
        AND (secondary_website_type NOT ILIKE '%pharmaceutical manufacturer%')
        AND (secondary_website_type NOT ILIKE '%psychoactive high%')
        AND (secondary_website_type NOT ILIKE '%smokes%')
        AND (secondary_website_type NOT ILIKE '%supplement%')
        AND (secondary_website_type NOT ILIKE '%telemedicine%')
        AND (secondary_website_type NOT ILIKE '%drug distributor%')
        AND (secondary_website_type NOT ILIKE '%steroid%')
        AND (secondary_website_type NOT ILIKE '%hijacked%')
        AND (network_names NOT ILIKE '%peptides%')
        AND (network_names NOT ILIKE '%research chemicals%')
        AND (network_names NOT ILIKE '%animal meds%')
    )
    
    OR (
        shopping_website_type = 'other' 
        AND website_status = 'inapplicable'
        AND (secondary_website_type NOT ILIKE '%cbd%')
        AND (secondary_website_type NOT ILIKE '%marijuana%')
        AND (secondary_website_type NOT ILIKE '%cosmetic%')
        AND (secondary_website_type NOT ILIKE '%pharmacy%')
        AND (secondary_website_type NOT ILIKE '%other healthcare%')
        AND (secondary_website_type NOT ILIKE '%paraphernalia%')
        AND (secondary_website_type NOT ILIKE '%pharmaceutical manufacturer%')
        AND (secondary_website_type NOT ILIKE '%psychoactive high%')
        AND (secondary_website_type NOT ILIKE '%smokes%')
        AND (secondary_website_type NOT ILIKE '%supplement%')
        AND (secondary_website_type NOT ILIKE '%telemedicine%')
        AND (secondary_website_type NOT ILIKE '%drug distributor%')
        AND (secondary_website_type NOT ILIKE '%steroid%')
        AND (secondary_website_type NOT ILIKE '%hijacked%')
        AND (network_names NOT ILIKE '%peptides%')
        AND (network_names NOT ILIKE '%research chemicals%')
        AND (network_names NOT ILIKE '%animal meds%')
        AND (website_ever_rogue <> 'yes')
    )
    
    OR (
        shopping_website_type = 'third party' 
        AND click_url NOT ILIKE '%google.com%'
        AND secondary_website_type = 'search engine'
        AND (website_ever_rogue <> 'yes' )
    )
    
    OR click_url ILIKE '%.blinkhealth.com%'
    OR click_url ILIKE '%.blueberrypharmacy.com%'
    OR click_url ILIKE '%.bluechew.com%'
    OR click_url ILIKE '%.getzealthy.com%'
    OR click_url ILIKE '%.sonapharmacy.com%'
    OR click_url IN (
        'booksamillion.com',
        'calpaclab.com',
        'ipharmachine.com',
        'labproservices.com',
        'propartsusa.com',
        'usapartsonline.com',
        'wheelsbelowretail.com',
        'wheelwarehouse.com'
    )
);

