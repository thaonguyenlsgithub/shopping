---

---- INCLUSION 


SELECT *
FROM shopping 
WHERE click_url NOT IN (
SELECT click_url from shopping 
WHERE  NOT (
    -- Row 0: alcohol + exclusions
    (shopping_website_type = 'alcohol' 
     AND secondary_website_type NOT LIKE '%cbd%'
     AND secondary_website_type NOT LIKE '%marijuana%'
     AND secondary_website_type NOT LIKE '%cosmetic%'
     AND secondary_website_type NOT LIKE '%pharmacy%'
     AND secondary_website_type NOT LIKE '%other healthcare%'
     AND secondary_website_type NOT LIKE '%paraphernalia%'
     AND secondary_website_type NOT LIKE '%pharmaceutical manufacturer%'
     AND secondary_website_type NOT LIKE '%psychoactive high%'
     AND secondary_website_type NOT LIKE '%cbd%'
     AND secondary_website_type NOT LIKE '%smokes%'
     AND secondary_website_type NOT LIKE '%supplement%'
     AND secondary_website_type NOT LIKE '%telemedicine%'
     AND secondary_website_type NOT LIKE '%drug distributor%'
     AND secondary_website_type NOT LIKE '%steroid%'
     AND secondary_website_type NOT LIKE '%hijacked%'
     AND network_names NOT LIKE '%peptides%'
     AND network_names NOT LIKE '%research chemicals%'
     AND network_names NOT LIKE '%animal meds%')
    
    -- Row 1: dating + exclusions
    OR (shopping_website_type = 'dating' 
        AND secondary_website_type NOT LIKE '%cbd%'
        AND secondary_website_type NOT LIKE '%marijuana%'
        AND secondary_website_type NOT LIKE '%cosmetic%'
        AND secondary_website_type NOT LIKE '%pharmacy%'
        AND secondary_website_type NOT LIKE '%other healthcare%'
        AND secondary_website_type NOT LIKE '%paraphernalia%'
        AND secondary_website_type NOT LIKE '%pharmaceutical manufacturer%'
        AND secondary_website_type NOT LIKE '%psychoactive high%'
        AND secondary_website_type NOT LIKE '%cbd%'
        AND secondary_website_type NOT LIKE '%smokes%'
        AND secondary_website_type NOT LIKE '%supplement%'
        AND secondary_website_type NOT LIKE '%telemedicine%'
